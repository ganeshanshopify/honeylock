.cr-boundary{
  .cr-viewport{
    {% comment %} position: relative;
    top:50%;
    transform:rotate(-45deg) translate(0%,-50%); {% endcomment %}
    width: 258px;
    height: 258px;

  }
}
.heart-shaped {
  position: relative;
  top:50%;
  transform:rotate(-45deg) translate(0%,-50%);
  border: solid 2px #fff;
  width:140px;
  height: 140px;    
  &:before{
    content:'';
    position:absolute;
    top: -50%;
    left:0px;
    width:230px;
    height: 230px;
    border-radius: 50%;
    border:2px solid #fff;
  }
}
.shape-box {
  height: 504px;
  width: 504px;
  position: relative;
  margin: 100px;
  overflow: hidden;
}
.shape-box:before,
.shape-box:after {
  content: '';
  display: block;
  height: 100px;
  width: 120px;
  background: #ffebe7;
  transform: rotate(45deg);
  left: 58px;
  position: absolute;
  top: -90px;
}
.shape-box:after {
  {% comment %} width: 760px;
  height: 750px;
  box-shadow: inset 0 0 0 220px #2B2B2B;
  top: -150px;
  left: -130px;
  background: none; {% endcomment %}
  width: 763px;
  height: 690px;
  box-shadow: inset 0 0 0 220px #ffebe7;
  top: -260px;
  left: -277px;
  background: none;
}
.heart {
  transform: rotate(45deg);
  height: 292px;
  width: 292px;
  box-shadow: inset 0 0 0 38px #ffebe7;
  position: absolute;
  left: -17px;
  top: -53px;
}
.heart:before,
.heart:after {
    content: '';
    display: block;
    width: 129px;
    height: 129px;
    border-radius: 50%;
    box-shadow: -40px -15px 0 20px #ffebe7;
    position: absolute;
    left: 38px;
    top: 127px;
}
.heart:after {
  box-shadow: -15px -40px 0 21px #ffebe7;
  left: 127px;
  top: 38px;
}
.croppie-container .cr-viewport {
      box-shadow: 0 0 2000px 2000px #ffebe7;
      border: 0;
}
.shape-box-cart {
  display: none;
  margin: 0;
}