/**
 * Article
 */

.article--single {
  padding: $gutter 0;
  position: relative;

  @include media-query($small) {
    padding-top: 0;
  }

  .article {
    position: relative;
    margin-bottom: 30px;
    @include media-query($small) {
      margin-top: 63px;
    }
  }

  .article__title {
    font-size: 2.46rem;
    font-weight: 800;
    margin: 11px 0 30px;
  }

  .article__meta {
    @extend h6;
    font-size: 0.8rem;
    display: flex;
    flex-wrap: wrap;
    padding: 7px 0;
    opacity: 1;
  }

  .article__image {
    margin: -$gutter 0 $gutter;
    background-position: center center;
    background-repeat: no-repeat;
    background-size: cover;
  }

  @include media-query($small) {
    .article {
      margin-left: -13px;
      margin-right: -13px;
    }

    .article__title {
      font-size: 26px;
      line-height: 31px;
      margin: 0;
    }

    .article__meta {
      padding: 0;
      margin: 0;
      font-size: 14px;
    }

    .article__image {
      margin: 0 0 -22px;
    }

    .hr {
      margin: 10px 0;
    }
  }
