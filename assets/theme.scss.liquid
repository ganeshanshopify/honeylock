    }
  }
  .cart-customize-product-tabs__categories {
    margin-left: 0;
    display: flex;
    justify-content: center;
  }
}
@media (max-width: 767px) {
  .upload-image-pendant--croppie {
    align-items: center;
  }
  
}
.cart-customize-product-tabs__categories {
  border-bottom: solid 1px #F2F2F2;
}
.cart-customize-product-tabs__categories li {
  padding: 30px;
  color: #707070;
  font-size: 18px;
  line-height: 26px;
  pointer-events: none;
  cursor: not-allowed;
}

.cart-customize-product-tabs__categories li.active {
  font-weight: bold;
  color: #000;
}
.cart-customize-product-tabs__content li {
  display: none;
}
.cart-customize-product-tabs__content li.active {
  display: block;
}
.choose-image-heading {
  font-size: 18px;
  line-height: 28px;
  margin-bottom: 10px;
