}
.dark-overlay {
  width: 100vw;
  height: 100vh;
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background: #000;
  opacity: 0.4;
  z-index: 9998;
  display: none;
  visibility: none;
  -webkit-transition: all 1s ease;
  -ms-transition: eall 1s ease;
}
.cart-customize-product-tabs {
  margin-top: 15px;
}

.size-guide-product-list li.active {
