.swatch {
  display: flex;
  flex-wrap: wrap;
  align-items: flex-start;
  margin: 0;
  flex: 0 1 100%;

   .text-center & {
    justify-content: center;
  }

   .header {
    width: 100%;
    font-size: 12px * $font-adjust-body;
    line-height: normal;
    padding: 2px 0;
    margin: 0 0 8px;
    display: block;
  }

   input {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
    margin: 0;
    border-radius: 10%;
    z-index: 2;
    cursor: pointer;

     &:checked + label {
      &::after {
        opacity: 1;
        visibility: visible;
        transition: opacity .2s ease, visibility 0s linear 0s;
        @include prefix(transform, translateY(0), webkit spec);
      }
    }
  }

   label {
    position: relative;
    float: left;
    height: $swatch_height !important;
    min-width: $swatch_width !important;
    margin: 0;
    text-align: center;
    line-height: $swatch_height;
    white-space: nowrap;
    text-transform: uppercase;
    display: flex;
    align-items: center;
    justify-content: center;

     span {
      position: absolute;
      width: 100%;
      height: 100%;
      border-radius: 50%;
      background-color: #ddd;
      background-position: center center;
      background-size: cover;
      background-repeat: no-repeat;
    }

     &::after {
      content: '';
      position: absolute;
      top: -3px;
      left: -3px;
      right: -3px;
      bottom: -3px;
      border: 1px solid rgba($color_text_light, 0.5);
      border-radius: 50%;
      opacity: 0;
      visibility: hidden;
      transition: opacity .2s ease, visibility 0s linear .2s;
    }
  }

   .swatch-element {
    position: relative;
    margin: 0 10px 10px 0;
    -webkit-transform: translateZ(0); /* webkit flicker fix */
    -webkit-font-smoothing:antialiased; /* webkit text rendering fix */

     label {
      padding: 0 10px;
      cursor: pointer;
    }

     .crossed-out {
      display: none;
    }

     &.color {
      label {
        padding: 0;
      }
    }

     &.soldout {
      .crossed-out {
        display: block;
      }

       label {
        filter: alpha(opacity=60); /* internet explorer */
        -khtml-opacity: 0.6;      /* khtml, old safari */
        -moz-opacity: 0.6;       /* mozilla, netscape */
        opacity: 0.6;           /* fx, safari, opera */
      }
    }

     &.white {
      label {
        span {
          box-shadow: inset 0 0 0 1px $color_site_border;
        }
      }
    }

     @include media-query( $small ) {
      float: none;
      display: inline-block;
      margin: 0 5px 10px;
    }

     &::after {
      content: attr( data-value );
      position: absolute;
      left: 0;
      top: 100%;
      margin-top: 8px;
      background: $color_bg;
      color: $color_text;
      padding: 2px 5px;
      border-radius: 3px;
      box-shadow: 0 1px 3px rgba(0, 0, 0, .15);
      border: 1px solid $color_site_border;
      white-space: nowrap;
      opacity: 0;
      visibility: hidden;
      transition: transform .15s ease, opacity .15s ease, visibility 0s linear .15s;
      font-size: 12px * $font-adjust-body;
      @include prefix(transform, translateY(50%), webkit spec);
    }

     &:hover {
      z-index: 5;
      &::after {
        opacity: 1;
        visibility: visible;
        transition: transform .3s ease, opacity .3s ease, visibility 0s linear 0s;
        @include prefix(transform, translateY(0), webkit spec);
      }
    }
  }

   &.error {
    background-color: #E8D2D2!important;
    color: #333 !important;
    padding: 1em;

     p {
      margin: 0.7em 0;

       &:first-child {
        margin-top: 0;
      }

       &:last-child {
        margin-bottom: 0;
      }
    }

     code {
      font-family: monospace;
    }
  }
}


/*================ GLOBAL ================*/
/**
 * Reset
 */

html, body, div, span, object, iframe, p, blockquote, pre,
abbr, address, cite, code, del, dfn, img, ins, kbd, q, samp,
small, sub, sup, var, dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, figcaption, figure,
footer, header, hgroup, menu, nav, section, summary,
time, mark, audio, video {
  margin: 0;
  padding: 0;
  border: 0;
  font-size: 100%;
  font: inherit;
  vertical-align: baseline;
  }

article, aside, details, figcaption, figure,
footer, header, hgroup, menu, nav, section {
  display: block;
  }

blockquote, q { quotes: none; }
blockquote:before, blockquote:after,
q:before, q:after { content: ''; content: none; }
ins { background-color: #ff9; color: #000; text-decoration: none; }
mark { background-color: #ff9; color: #000; font-style: italic; font-weight: $font-weight-body-bold; }
del { text-decoration: line-through; }
abbr[title], dfn[title] { border-bottom: 1px dotted; cursor: help; }
table { border-collapse: collapse; border-spacing: 0; }
hr { display: block; height: 1px; border: 0; border-top: 1px solid #ccc; margin: 1em 0; padding: 0; }

html { overflow-y: scroll; }
ol { list-style-type: decimal; }
p { margin: 0 0 1em 0; }
nav ul, nav li { margin: 0; list-style:none; list-style-image: none; }
small { font-size: 85%; }
strong, th { font-weight: $font-weight-body-bold; }
td { vertical-align: top; }

sub, sup { font-size: 75%; line-height: 0; position: relative; }
sup { top: -0.5em; }
sub { bottom: -0.25em; }

pre { white-space: pre; white-space: pre-wrap; word-wrap: break-word; padding: 15px; }
textarea { overflow: auto; }
.ie6 legend, .ie7 legend { margin-left: -7px; }
input[type="radio"] { vertical-align: text-bottom; }
input[type="checkbox"] { vertical-align: bottom; }
.ie7 input[type="checkbox"] { vertical-align: baseline; }
.ie6 input { vertical-align: text-bottom; }
input[type="button"], input[type="submit"], input[type="image"], button { cursor: pointer; }
button, input, select, textarea { margin: 0; }
select{ max-width: 99%; /* for ff */ }
input[type="text"].error, input[type="email"].error, input[type="password"].error { border-color: #dd0000; }
input[type="text"], input[type="password"], input[type="email"], input[type="tel"]{ -webkit-appearance: none; -moz-appearance: none; appearance: none;}

/**
 * Helpers
 */

.clearfix {
  @include clearfix();
}

.visually-hidden {
  @include visually-hidden();
}

.js-focus-hidden:focus {
  outline: none;
}

.label-hidden {
  @include visually-hidden();

  // No placeholders, so force show labels
  .no-placeholder & {
    @include visually-shown();
  }
}

.visually-shown {
  @include visually-shown();
}

// Only show when JS is not supported
.no-js:not(html) {
  display: none;

  .no-js & {
    display: block;
  }
}

// Only show when JS is supported
.js {
  .no-js & {
    display: none;
  }
}

// Only show when browser cookies are not supported
.supports-no-cookies:not(html) {
  display: none;

  html.supports-no-cookies & {
    display: block;
  }
}

// Only show when browser cookies are supported
.supports-cookies {
  html.supports-no-cookies & {
    display: none;
  }
}

/*================ Responsive Show/Hide Helper ================*/
@mixin responsive-display-helper($grid-breakpoint-type: '') {
  // sass-lint:disable no-important
  .#{$grid-breakpoint-type}show {
    display: block !important;
  }

  .#{$grid-breakpoint-type}hide {
    display: none !important;
  }
}

/*================ Responsive Text Alignment Helper ================*/
@mixin responsive-text-align-helper($grid-breakpoint-type: '') {
  // sass-lint:disable no-important
  .#{$grid-breakpoint-type}text-left {
    text-align: left !important;
  }

  .#{$grid-breakpoint-type}text-right {
    text-align: right !important;
  }

  .#{$grid-breakpoint-type}text-center {
    text-align: center !important;
  }
}

/*============================================================================
  Skip to content button
    - Overrides .visually-hidden when focused
==============================================================================*/
.skip-link:focus {
  @include visually-shown(absolute);
  color: $color_link;
  background-color: $color_bg;
  padding: 20px;
  z-index: 5000;
  transition: none;
}


/**
 * UTILS
 */
.ir { display: block; text-indent: -999em; overflow: hidden; background-repeat: no-repeat; text-align: left; direction: ltr; }
.hidden { display: none; visibility: hidden; }
.visuallyhidden { border: 0; clip: rect(0 0 0 0); height: 1px; margin: -1px; overflow: hidden; padding: 0; position: absolute; width: 1px; }
.visuallyhidden.focusable:active,
.visuallyhidden.focusable:focus { clip: auto; height: auto; margin: 0; overflow: visible; position: static; width: auto; }
.invisible { visibility: hidden; }
.clearfix:before, .clearfix:after { content: "\0020"; display: block; height: 0; overflow: hidden; }
.clearfix:after { clear: both; }
.clearfix { zoom: 1; }
.left{ float: left; }
.right{ float: right; }
.center { text-align: center; }
.align-left { text-align: left; }
.align-right { text-align: right; }
.gutter-top { padding-top: $gutter; }
.gutter-bottom { padding-bottom: $gutter; }


.mobile {
  display: none;
}
.mobile-small {
  display: none;
}

@include media-query($small) {
  .mobile {
    display: block;
  }
}
@media screen and (max-width: 480px) {
  .mobile-small {
    display: block;
  }
}

/**
 * Grid
 */

 .grid {
   @include clearfix();
   list-style: none;
   margin: 0;
   padding: 0;
   margin-left: -$grid-gutter;
   @include media-query($small) {
     margin-left: ($grid-gutter / -2);
   }
   @include media-query($medium) {
     margin-left: ($grid-gutter / -1.5);
   }
 }

 .grid__item {
   float: left;
   padding-left: $grid-gutter;
   width: 100%;
   @include media-query($small) {
     padding-left: ($grid-gutter / 2);
   }
   @include media-query($medium) {
     padding-left: ($grid-gutter / 1.5);
   }

   &[class*='--push'] {
     position: relative;
   }
 }

 /* Manual grid__item clearfix */
.grid__item.clear {
  clear: both;
}

 /*============================================================================
   Reversed grids allow you to structure your source in the opposite
   order to how your rendered layout will appear.
 ==============================================================================*/
 .grid--rev {
   direction: rtl;
   text-align: left;

   > .grid__item {
     direction: ltr;
     text-align: left;
     float: right;
   }
 }

 /* Gutterless grids have all the properties of regular grids, minus any spacing. */
 .grid--full {
   @extend .grid;
   margin-left: 0;

   > .grid__item {
     padding-left: 0;
   }
 }

 /*============================================================================
   Grid Columns
     - Create width classes, prepended by the breakpoint name.
 ==============================================================================*/
 // sass-lint:disable brace-style empty-line-between-blocks
 @mixin grid-column-generator($breakpoint: '') {
   /** Whole */
   .#{$breakpoint}one-whole { width: 100%; }

   /* Halves */
   .#{$breakpoint}one-half { width: percentage(1 / 2); }

   /* Thirds */
   .#{$breakpoint}one-third { width: percentage(1 / 3); }
   .#{$breakpoint}two-thirds { width: percentage(2 / 3); }

   /* Quarters */
   .#{$breakpoint}one-quarter { width: percentage(1 / 4); }
   .#{$breakpoint}two-quarters { width: percentage(2 / 4); }
   .#{$breakpoint}three-quarters { width: percentage(3 / 4); }

   /* Fifths */
   .#{$breakpoint}one-fifth { width: percentage(1 / 5); }
   .#{$breakpoint}two-fifths { width: percentage(2 / 5); }
   .#{$breakpoint}three-fifths { width: percentage(3 / 5); }
   .#{$breakpoint}four-fifths { width: percentage(4 / 5); }

   /* Sixths */
   .#{$breakpoint}one-sixth { width: percentage(1 / 6); }
   .#{$breakpoint}two-sixths { width: percentage(2 / 6); }
   .#{$breakpoint}three-sixths { width: percentage(3 / 6); }
   .#{$breakpoint}four-sixths { width: percentage(4 / 6); }
   .#{$breakpoint}five-sixths { width: percentage(5 / 6); }

   /* Eighths */
   .#{$breakpoint}one-eighth { width: percentage(1 / 8); }
   .#{$breakpoint}two-eighths { width: percentage(2 / 8); }
   .#{$breakpoint}three-eighths { width: percentage(3 / 8); }
   .#{$breakpoint}four-eighths { width: percentage(4 / 8); }
   .#{$breakpoint}five-eighths { width: percentage(5 / 8); }
   .#{$breakpoint}six-eighths { width: percentage(6 / 8); }
   .#{$breakpoint}seven-eighths { width: percentage(7 / 8); }

   /* Tenths */
   .#{$breakpoint}one-tenth { width: percentage(1 / 10); }
   .#{$breakpoint}two-tenths { width: percentage(2 / 10); }
   .#{$breakpoint}three-tenths { width: percentage(3 / 10); }
   .#{$breakpoint}four-tenths { width: percentage(4 / 10); }
   .#{$breakpoint}five-tenths { width: percentage(5 / 10); }
   .#{$breakpoint}six-tenths { width: percentage(6 / 10); }
   .#{$breakpoint}seven-tenths { width: percentage(7 / 10); }
   .#{$breakpoint}eight-tenths { width: percentage(8 / 10); }
   .#{$breakpoint}nine-tenths { width: percentage(9 / 10); }

   /* Twelfths */
   .#{$breakpoint}one-twelfth { width: percentage(1 / 12); }
   .#{$breakpoint}two-twelfths { width: percentage(2 / 12); }
   .#{$breakpoint}three-twelfths { width: percentage(3 / 12); }
   .#{$breakpoint}four-twelfths { width: percentage(4 / 12); }
   .#{$breakpoint}five-twelfths { width: percentage(5 / 12); }
   .#{$breakpoint}six-twelfths { width: percentage(6 / 12); }
   .#{$breakpoint}seven-twelfths { width: percentage(7 / 12); }
   .#{$breakpoint}eight-twelfths { width: percentage(8 / 12); }
   .#{$breakpoint}nine-twelfths { width: percentage(9 / 12); }
   .#{$breakpoint}ten-twelfths { width: percentage(10 / 12); }
   .#{$breakpoint}eleven-twelfths { width: percentage(11 / 12); }
 }

 /*================ Grid push classes ================*/
 @mixin grid-push-generator($breakpoint: '') {
   /* Halves */
   .#{$breakpoint}push-one-half { left: percentage(1 / 2); }

   /* Thirds */
   .#{$breakpoint}push-one-third { left: percentage(1 / 3); }
   .#{$breakpoint}push-two-thirds { left: percentage(2 / 3); }

   /* Quarters */
   .#{$breakpoint}push-one-quarter { left: percentage(1 / 4); }
   .#{$breakpoint}push-two-quarters { left: percentage(2 / 4); }
   .#{$breakpoint}push-three-quarters { left: percentage(3 / 4); }

   /* Fifths */
   .#{$breakpoint}push-one-fifth { left: percentage(1 / 5); }
   .#{$breakpoint}push-two-fifths { left: percentage(2 / 5); }
   .#{$breakpoint}push-three-fifths { left: percentage(3 / 5); }
   .#{$breakpoint}push-four-fifths { left: percentage(4 / 5); }

   /* Sixths */
   .#{$breakpoint}push-one-sixth { left: percentage(1 / 6); }
   .#{$breakpoint}push-two-sixths { left: percentage(2 / 6); }
   .#{$breakpoint}push-three-sixths { left: percentage(3 / 6); }
   .#{$breakpoint}push-four-sixths { left: percentage(4 / 6); }
   .#{$breakpoint}push-five-sixths { left: percentage(5 / 6); }

   /* Eighths */
   .#{$breakpoint}push-one-eighth { left: percentage(1 / 8); }
   .#{$breakpoint}push-two-eighths { left: percentage(2 / 8); }
   .#{$breakpoint}push-three-eighths { left: percentage(3 / 8); }
   .#{$breakpoint}push-four-eighths { left: percentage(4 / 8); }
   .#{$breakpoint}push-five-eighths { left: percentage(5 / 8); }
   .#{$breakpoint}push-six-eighths { left: percentage(6 / 8); }
   .#{$breakpoint}push-seven-eighths { left: percentage(7 / 8); }

   /* Tenths */
   .#{$breakpoint}push-one-tenth { left: percentage(1 / 10); }
   .#{$breakpoint}push-two-tenths { left: percentage(2 / 10); }
   .#{$breakpoint}push-three-tenths { left: percentage(3 / 10); }
   .#{$breakpoint}push-four-tenths { left: percentage(4 / 10); }
   .#{$breakpoint}push-five-tenths { left: percentage(5 / 10); }
   .#{$breakpoint}push-six-tenths { left: percentage(6 / 10); }
   .#{$breakpoint}push-seven-tenths { left: percentage(7 / 10); }
   .#{$breakpoint}push-eight-tenths { left: percentage(8 / 10); }
   .#{$breakpoint}push-nine-tenths { left: percentage(9 / 10); }

   /* Twelfths */
   .#{$breakpoint}push-one-twelfth { left: percentage(1 / 12); }
   .#{$breakpoint}push-two-twelfths { left: percentage(2 / 12); }
   .#{$breakpoint}push-three-twelfths { left: percentage(3 / 12); }
   .#{$breakpoint}push-four-twelfths { left: percentage(4 / 12); }
   .#{$breakpoint}push-five-twelfths { left: percentage(5 / 12); }
   .#{$breakpoint}push-six-twelfths { left: percentage(6 / 12); }
   .#{$breakpoint}push-seven-twelfths { left: percentage(7 / 12); }
   .#{$breakpoint}push-eight-twelfths { left: percentage(8 / 12); }
   .#{$breakpoint}push-nine-twelfths { left: percentage(9 / 12); }
   .#{$breakpoint}push-ten-twelfths { left: percentage(10 / 12); }
   .#{$breakpoint}push-eleven-twelfths { left: percentage(11 / 12); }
 }

 /*================ Clearfix helper on uniform grids ================*/
 @mixin grid-uniform-clearfix($breakpoint: '') {
   .grid--uniform {
     .#{$breakpoint}one-half:nth-child(2n+1),
     .#{$breakpoint}one-third:nth-child(3n+1),
     .#{$breakpoint}one-quarter:nth-child(4n+1),
     .#{$breakpoint}one-fifth:nth-child(5n+1),
     .#{$breakpoint}one-sixth:nth-child(6n+1),
     .#{$breakpoint}two-sixths:nth-child(3n+1),
     .#{$breakpoint}three-sixths:nth-child(2n+1),
     .#{$breakpoint}one-eighth:nth-child(8n+1),
     .#{$breakpoint}two-eighths:nth-child(4n+1),
     .#{$breakpoint}four-eighths:nth-child(2n+1),
     .#{$breakpoint}five-tenths:nth-child(2n+1),
     .#{$breakpoint}one-twelfth:nth-child(12n+1),
     .#{$breakpoint}two-twelfths:nth-child(6n+1),
     .#{$breakpoint}three-twelfths:nth-child(4n+1),
     .#{$breakpoint}four-twelfths:nth-child(3n+1),
     .#{$breakpoint}six-twelfths:nth-child(2n+1) { clear: both; }
   }
 }
 // sass-lint:enable brace-style empty-line-between-blocks

 /*================ Build Base Grid Classes ================*/
 @include grid-column-generator();
 @include responsive-display-helper();
 @include responsive-text-align-helper();

 /*================ Build Responsive Grid Classes ================*/
 @each $breakpoint in $breakpoint-has-widths {
   @include media-query($breakpoint) {
     @include grid-column-generator('#{$breakpoint}--');
     @include grid-uniform-clearfix('#{$breakpoint}--');
     @include responsive-display-helper('#{$breakpoint}--');
     @include responsive-text-align-helper('#{$breakpoint}--');
   }
 }

 /*================ Build Grid Push Classes ================*/
 @each $breakpoint in $breakpoint-has-push {
   @include media-query($breakpoint) {
     @include grid-push-generator('#{$breakpoint}--');
   }
 }

/**
 * Layout
 */

html,body {
  min-height: 100%;
}
* {
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

html {
  font-size: $font-size-base;
  body {
    color: $color_text;
    background-color: $color_bg;
    font-family: $font-stack-body;
    font-size: $font-size-base;

    * {
      -webkit-font-smoothing: antialiased;
    }
  }
}

html.mobile-menu--open {
  // @include media-query(small) {
    overflow: hidden !important;
    // position: fixed;
    // width: 100%;
  // }
}

.container {
  margin:0;
  padding:0;
  display:block;
  width:100%;
  min-height:100%;
  background-color: $color_bg;
}
.content-container {
  margin: auto;
  max-width: 1100px;
}
#IE8bg {
  display: none;
}
// Used for inline-block grids
.just {
  text-align: justify;
  font-size: 0.01em;
}
.product-grid.just {
  font-size: 0.001px;
}
.just > * {
  display: inline-block;
  text-align: left;
  vertical-align: top;

  // RESET AFTER INLINE_BLOCK GRID
  font-size: $font-size-base;
}
.just:after {
  content:'';
  display: inline-block;
  width: 100%;
  height: 0;
}

/**
 * CONTENT WRAPPER
 */
.main-content {
  margin: 0;
  padding: 0 0 40px 0;
  min-width: 100%;
  display: block;
  overflow: hidden;
  @include media-query($medium-up) {
    padding-top: 0 !important;
  }

  @include media-query($small) {
    padding-top: 46px;
    padding-bottom: 0;
  }
}
.template-product .main-content,
.template-index .main-content,
.template-collection .main-content,
.template-list-collections .main-content,
.template-search .main-content {
  @include media-query($medium-up) {
    padding: 0;
  }
}

.wrapper {
  @include clearfix();
  max-width: $width-site;
  margin: 0 auto;
  padding: 0 $gutter;

  @include media-query($small) {
    padding: 0 ($gutter / 2);
  }
}

.wrapper--full {
  max-width: none;
  margin: 0 10px;

  @include media-query ($medium) {
    padding: 0 20px;
  }

  @include media-query($small) {
    padding: 0 ($gutter / 2);
    margin: 0;
  }
}

.page-margin{
  margin-top: 60px;
  @include media-query($medium) {
    padding-bottom: 40px;
  }
  @include media-query($small) {
    padding-bottom: 24px;
  }
}


/**
 * TABLES
 */
table {
  border-collapse: collapse;
  border-spacing: 0;
  border: 1px solid $color_site_border;
  margin: 1em 0;
}
table tr > th,
table tr > td {
  border-bottom: 1px solid $color_site_border;
  border-right: 1px solid $color_site_border;
  padding: 10px;
  line-height: 1.3;
  vertical-align: middle;
}
.table {
  display: table;
  width: 100%;
  height: 100%;
}
dl {
  margin-bottom: 1.5em;
}
dt,
dd {
  line-height: 1.5em;
}
dt {
  font-weight: $font-weight-body-bold;
}
dd {
  margin-left: .8em;
}

/**
 * Typography
 */


h1, h2, h3, h4, h5, h6 {
  font-family: $font-stack-heading;
  line-height: 1.2;
  font-weight: $font-weight-heading;
  margin: 0 0 .5em 0;

  &.theme {
    text-align: center;
    position: relative;
    z-index: 11;
  }
}
h1, .h1 {
  font-size: 2.5rem * $font-adjust-heading;
  line-height: 1.1;
  @include media-query($small){
    font-size: 2rem * $font-adjust-heading;
  }
}
h2, .h2 {
  font-size: 2rem * $font-adjust-heading;
  line-height: 1.2;
  @include media-query($small){
    font-size: 1.8rem * $font-adjust-heading;
  }
}
h3, .h3 {
  font-size: 1.8rem * $font-adjust-heading;
  line-height: 1.3;
  @include media-query($small){
    font-size: 1.5rem * $font-adjust-heading;
  }
}
h4, .h4 {
  font-size: 1.5rem * $font-adjust-heading;
  line-height: 1.5;
  @include media-query($small){
    font-size: 1.35rem * $font-adjust-heading;
  }
}
h5, .h5 {
  font-size: 1.25rem * $font-adjust-heading;
  line-height: 1.6;
}
h6, .h6 {
  font-size: .875rem * $font-adjust-heading;
  line-height: 1.6;
}

/*================ Headings with body font ================*/
.body{
  font-family: $font-stack-body;
  font-weight: $font-weight-body;
  font-style: $font-style-body;
}
.h1--body {
  font-size: 2rem * $font-adjust-body;
  line-height: 1.1;
  @include media-query($small){
    font-size: 2.815rem * $font-adjust-body;
  }
}
.h2--body {
  font-size: 1.675rem * $font-adjust-body;
  line-height: 1.54;
  @include media-query($small){
    font-size: 1.8rem * $font-adjust-body;
  }
}
.h3--body {
  font-size: 1.5rem * $font-adjust-body;
  line-height: 1.3;
  @include media-query($small){
    font-size: 1.875rem * $font-adjust-body;
  }
}
.h4--body {
  font-size: 1.5rem * $font-adjust-body;
  line-height: 1.5;
}
.h5--body {
  font-size: 1.25rem * $font-adjust-body;
  line-height: 1.6;
}
.h6--body {
  font-size: .875rem * $font-adjust-body;
  line-height: 1.6;
}
.h1--body,
.h2--body,
.h3--body,
.h4--body,
.h5--body,
.h6--body{
  @extend .body;
  text-transform: none;
  letter-spacing: 0px;
  margin: 0 0 0.75em;
  line-height: 1.4;
  a {
    text-decoration: none;
    font-weight: inherit;
  }
}

/*================ Standard elements ================*/
p,
.p {
  font-size: $font-size-base;
  font-weight: $font-weight-body;
  line-height: 1.5;
  margin: 0 0 0.5em;
}
.strong, strong{
  font-weight: $font-weight-body-bold;
}
small, .small {
  font-size: 0.875em;
  line-height: 1.5;
}
a {
  text-decoration: none;
  color: $color_link;
}
a:hover {
  color: $color_link_hover
}
blockquote {
  padding-left: 1.5em;
  border-left: 5px solid $color_link;
  margin: 0 0 1.5em 1.25em;
}
ul, ol{
  margin: 0 0 1.5em 1.3em;
  &.unstyled{
    list-style-type: none;
    margin-left: 0;
  }
  &.inline-list li {
    display: inline-block;
    margin-bottom: 0;
  }
}
li{
  margin: 0 0 0.25em 0;
}
hr, .hr {
  clear: both;
  border-top: solid $color_site_border;
  border-width: 1px 0 0;
  margin: rem(20px) 0;
  height: 0;
  &.hr--small {
    margin: rem(10px) 0;
  }
  &.hr--tall {
    margin: rem(40px) 0;
  }
  &.hr--clear {
    border-top-color: transparent;
  }
  &.hr--dash {
    margin: $gutter auto;
    width: 40px;
  }
  &.hr--thick {
    border-width: 2px 0 0;
  }
  &.hr--underline{
    margin-top: 10px;
    margin-bottom: 20px;
  }
  &.hr--no-margin {
    margin: 0;
  }
}

.caps {
  text-transform: uppercase;
  letter-spacing: 2px;
  font-size: 0.8em * $font-adjust-body;
  font-family: $font-stack-body;
  font-weight: $font-weight-body !important;
}

.coll-heading{
  margin: 0;
  padding: 40px 0;
  font-weight: $font-weight-heading;
  .icon {
    width: 36px;
    height: 36px;
    margin-right: 12px;
  }
  @include media-query($medium){
    padding: 30px 0;
  }
  @include media-query($small){
    padding: 15px 0;
  }
}

/**
 * Icons
 */

.icon {
  display: inline-block;
  width: 20px;
  height: 20px;
  vertical-align: middle;
  fill: inherit;

  .no-svg & {
    display: none;
  }
}

.icon--wide {
  width: 40px;
  height: 40px;
}

.oi.small{
  font-size: 0.7em;
  margin-left: 8px;
}

svg,
symbol {
  &.icon:not(.icon--full-color) {
    circle,
    ellipse,
    g,
    line,
    path,
    polygon,
    polyline,
    rect {
      fill: inherit;
      stroke: inherit;
    }
  }
}

/*============================================================================
  A generic way to visually hide content while
  remaining accessible to screen readers (h5bp.com)
==============================================================================*/
.icon-fallback-text {
  @include visually-hidden();

  .no-svg & {
    @include visually-shown(static);
  }
}

/*================ Payment Icons ================*/
.payment-icons {
  @include prefix('user-select', 'none', moz ms webkit spec);
  cursor: default;
}

/**
 * Rich Text Editor
 */


.rte {
  img {
    height: auto;
  }

  table {
    table-layout: fixed;
  }

  ul,
  ol {
    margin: 0 0 ($gutter / 2) $gutter;
  }

  // Match the styles from RTE nested lists
  ul {
    list-style: disc outside;

    ul {
      list-style: circle outside;

      ul {
        list-style: square outside;
      }
    }
  }
}

.text-center.rte,
.text-center .rte {
  ul,
  ol {
    margin-left: 0;
    list-style-position: inside;
  }
}

// allow table to scroll for tables in the RTE since we don't know
// how many columns they will contain. Class added by JS.
// sass-lint:disable no-misspelled-properties
.rte-table {
  max-width: 100%;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}

// This class is wrapped around YouTube/Vimeo embeds in the RTE
// to make them responsive and prevent layout breaking
.rte__video-wrapper {
  position: relative;
  overflow: hidden;
  max-width: 100%;
  padding-bottom: 56.25%;
  height: 0;
  height: auto;

  iframe {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
  }
}

// This class is wrapped around tables in the RTE
// to prevent layout breaking with a scrollable parent
.rte__table-wrapper {
  max-width: 100%;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}

/**
 * Responsive tables
 * defined with .responsive-table on table element.
 */

@include media-query($small) {
  .responsive-table {
    width: 100%;

    thead {
      display: none;
    }

    tr {
      display: block;
    }

    // IE9 table layout fixes
    tr,
    td {
      float: left;
      clear: both;
      width: 100%;
    }

    th,
    td {
      display: block;
      text-align: right;
      padding: $gutter / 2;
      margin: 0;
    }

    td::before {
      content: attr(data-label);
      float: left;
      text-align: center;
      padding-right: 10px;
    }
  }

  // Add a keyline between rows
  .responsive-table-row + .responsive-table-row,
  tfoot > .responsive-table-row:first-child {
    position: relative;
    margin-top: 10px;
    padding-top: $gutter / 2;

    &::after {
      content: '';
      display: block;
      position: absolute;
      top: 0;
      left: $gutter / 2;
      right: $gutter / 2;
      border-bottom: 1px solid $color_site_border;
    }
  }
}

/**
 * Links & Buttons
 */

button {
  width: auto;
  overflow: visible;
}

.btn,
.button,
button {
  @include prefix('user-select', 'none', moz ms webkit spec);
  @include prefix(appearance, none, webkit moz spec);
  line-height: 1.5;
  transition: all 0.15s ease;
  display: inline-block;
  width: auto;
  text-decoration: none;
  text-align: center;
  vertical-align: middle;
  white-space: nowrap;
  border: 0;
  border-radius: $radius;
  font-weight: $font-weight-body-bold;
  cursor: pointer;
  background: $color_accent_bright;
  color: #fff;
  padding: 7px 14px;
  @include media-query($small) {
    padding: 5px 10px;
  }

  &:hover {
    color: #fff;
    background: darken($color_accent_bright, 5%);
    @include box_shadow(2, $color_accent_bright);
  }

  &:active{
    transform: translateY(1px);
  }
  &:focus {
    color: #fff;
    background: darken($color_accent_bright, 10%);
    outline: none;
  }
}
.btn--secondary{
  background-color: $transparent;
  color: $color_accent_bright;
  border: 1px solid $color_accent_bright;
  box-shadow: none;
  &:hover,
  &:active,
  &:focus {
    background-color: $color_accent_bright;
    color: opposite-color($color_accent_bright);
    border: 1px solid $color_accent_bright;
  }
  &:active{
    transform: translateY(1px);
    border: 1px solid darken($color_accent_bright, 5%);
  }
}

.btn--neutral{
  background-color: $color_text;
  color: $color_bg;
    @include box_shadow(2, $color_text);
  &:hover {
    color: $color_bg;
    @include box_shadow(3, $color_text);
    background: darken($color_text, 5%);
  }
  &:active,
  &:focus {
    background: darken($color_text, 10%);
    color: $color_bg;
    @include box_shadow(1, $color_text);
  }
}
.btn--secondary.btn--neutral{
  background-color: $transparent;
  color: $color_text;
  border: 1px solid $color_text;
  box-shadow: none;
  &:hover,
  &:active,
  &:focus {
    background-color: $color_text;
    color: #fff;
    border: 1px solid $color_text;
    box-shadow: none;
  }
}

.btn--accent{
  background-color: $color_accent_secondary;
  color: $color_accent_secondary_opposite;
  &:hover {
    color: $color_accent_secondary;
    @include box_shadow(2, $color_accent_secondary);
  }
  &:active,
  &:focus {
    background-color: #fff;
    color: $color_accent_secondary;
  }
}
.btn--secondary.btn--accent{
  background-color: $transparent;
  color: $color_accent_secondary;
  border: 1px solid $color_accent_secondary;
  box-shadow: none;
  &:hover,
  &:active,
  &:focus {
    background-color: $color_accent_secondary;
    color: #fff;
    border: 1px solid $color_accent_secondary;
    box-shadow: none;
  }
}

.btn--white {
  background-color: #fff;
  color: $color_text_dark;
  &:hover {
    color: $color_text;
    background-color: #fff;
    @include box_shadow(1, $color_text);
  }
  &:active,
  &:focus {
    background-color: #fff;
    color: $color_text;
  }
}
.btn--secondary.btn--white{
  background-color: transparent;
  color:  #fff;
  border: 1px solid #fff;
  box-shadow: none;
  &:hover,
  &:active,
  &:focus {
    background-color: #fff;;
    color: $color_text;
    border: 1px solid #fff;
    box-shadow: none;
  }
}

.btn.sml,
.buttom.sml {
  @extend .small;
  padding: 5px 10px;
}

.btn--unstyled {
  @include prefix('user-select', 'none', moz ms webkit spec);
  @include prefix(appearance, none, webkit moz spec);
  display: inline-block;
  width: auto;
  text-decoration: none;
  text-align: center;
  vertical-align: middle;
  white-space: nowrap;
}

.btn--half {
  margin: 0;
  min-width: 50%;
  @include media-query($small) {
    width: 100%;
  }
}

.btn--large {
  padding: 14px 28px;
  @include media-query($small) {
    padding: 10px 20px;
  }
}

.btn--small {
  font-size: 0.875em * $font-adjust-body;
  padding: 5px 10px;
  @include media-query($small) {
    padding: 4px 8px;
  }
}

.btn--full {
  width: 100%;
}

.btn--pill{
  border: none;
  padding: 3px 10px;
  background-color: $color_site_border_light;
  color: $color_text;
  border-radius: 20px;
  box-shadow: none;
  transition: all .15s ease;
  font-size: rem(14px * $font-adjust-body);
  &:hover {
    box-shadow: none;
    background-color: $color_site_border;
    color: $color_text;
  }
  &:active,
  &:focus {
    background-color: $color_site_border;
    box-shadow: none;
    outline: none;
  }
  .oi{ margin-left: 6px; }
}

.text-link {
  border-left: none;
  border-right: none;
  border-top: none;
  border-radius: 0;
  background-color: rgba(255, 255, 255, 0);
  text-decoration: none !important;
  cursor: pointer;
  color: $color_text;
  border-bottom: 1px solid $color_site_border_light;
  padding: 0 0 3px;
  transition: all 0.25s ease-in;

  &:hover {
    background-color: rgba(255, 255, 255, 0);
    color: $color_text;
    border-bottom: 1px solid darken($color_site_border_light, 10%);
    padding-bottom: 1px;
    margin-bottom: 2px;
    box-shadow: none;
  }

  &:active,
  &:focus {
    background-color: rgba(255, 255, 255, 0);
    color: $color_text;
    transform: translateY(2px);
    outline: none;
    border-bottom: 1px solid darken($color_site_border_light, 10%);
    border-left: none;
    border-right: none;
    border-top: none;
    box-shadow: none;
  }
}

.btn + .text-link,
button + .text-link {
  margin-left: 15px;
}

.inline-nav{
  list-style: none;
  list-style-type: none;
  text-align: center;
  padding: 0;
  margin: 0 0 30px 0;
  li{
    display: inline-block;
    padding: 5px 0;
    margin: 5px 20px;
    @include media-query($small){
      margin: 5px 15px 5px 0;
    }
  }
  li a{
    color: $color_link;
    position: relative;
    &:after{
      content: "";
      margin: auto;
      width: 10%;
      height: 2px;
      position: absolute;
      bottom: -4px;
      left: 0;
      opacity: 0;
      background: $color_link_hover;
      transition: all 0.15s ease-in;
      @include box_shadow(1, $color_link_hover);
    }
  }
  li.inline-nav--active a,
  li a:hover{
    color: $color_link;
  }
  li.inline-nav--active a:after,
  li a:hover:after{
    color: $color_link;
    opacity: 1;
    width: 100%;
  }
}

.input-group.newsletter-form{
  border: 1px solid $color_accent_secondary;
  .input-group-btn,
  .input-group-btn .btn,
  .input-group-btn .btn:active,
  .input-group-btn .btn:focus{
    background-color: $color_accent_secondary;
    color: $color_accent_secondary_opposite;
  }
  input{
    color: $color_text;
    border: none;
    &:active,
    &:focus{
      border:none;
      box-shadow: none;
    }
  }
}
.input-group.newsletter-form .input-group-btn .btn{
  border-radius: $radius;
}

/**
 * Forms
 */

input,
textarea,
select,
.input-group {
  border: 1px solid $color_site_border;
  font-size: 1em * $font-adjust-body;
  border-radius: $radius;
  max-width: 100%;
  padding: 9px 16px;
  margin: 7px 0;
  background: rgba(255,255,255,0);
  font-family: $font-stack-body;
  font-weight: $font-weight-body;
  font-style: $font-style-body;
  @include media-query($small){
    padding: 9px 10px;
    font-size: 16px;
  }
  &:focus {
    border: 1px solid $color_link;
    outline: none;
  }
  &[disabled] {
    cursor: default;
    background-color: $color_disabled;
    border-color: $color_disabled-dark;
  }
}
textarea {
  min-height: 100px;
  padding: 16px;
}

.form-item,
.form-field{
  width: 100%;
  max-width: 400px;
  margin: 0 auto 20px;
  line-height: 1;
  text-align: left;
  position: relative;
}
.form-item{
  select{
    width: 100%;
  }
}
.form-field{
  label {
    background: $color_bg;
    color: $color_text;
    font-size: 12px;
    font-weight: $font-weight-body-bold;
    left: 9px;
    opacity: 0;
    padding: 0 .5em;
    position: absolute;
    top: 2em;
    transition: all 0.15s ease-in;
    z-index: -1;
  }
  label.label--float{
    opacity: 1;
    top: -0.8em;
    z-index: 1;
  }
}
.form-field input[type="text"],
.form-field input[type="password"],
.form-field input[type="email"],
.form-field input[type="tel"],
.form-field textarea {
  display: block;
  width: 100%;
  outline: none;
}

input[type="checkbox"] {
  margin: 4px 3px 2px 0;
  -webkit-appearance: checkbox;
  -moz-appearance: checkbox;
  -ms-appearance: checkbox;
  -o-appearance: checkbox;
  appearance: checkbox;
}

input[type="radio"] {
  margin: 11px 3px 4px 0px;
  -webkit-appearance: radio;
  -moz-appearance: radio;
  -ms-appearance: radio;
  -o-appearance: radio;
  appearance: radio;
}

legend{
  padding-bottom: 6px;
}

.label--block{
  display: block;
}

/*================ Input Group ================*/
.input-group {
  background-color: $transparent;
  position: relative;
  display: table;
  border-collapse: separate;
  padding: 0;
  .btn{
    box-shadow: none;
  }
  input,
  input:focus,
  input:active{
    background-color: $transparent;
    border: none;
    box-shadow: none;
    margin: 0;

    // Nasty Firefox hack for inputs http://davidwalsh.name/firefox-buttons
    &::-moz-focus-inner {
      border: 0;
      padding: 0;
      margin-top: -1px;
      margin-bottom: -1px;
    }
  }
}
.input-group-field,
.input-group-btn {
  display: table-cell;
  vertical-align: middle;
  margin: 0;
}
.input-group .input-group-field {
  width: 100%;
  margin-bottom: 0;
}
.input-group-btn {
  position: relative;
  white-space: nowrap;
  width: 1%;
  padding: 0;
  margin: 0
}

/*================ Custom select style ================*/
select {
  @include prefix(appearance, none, webkit moz spec);
  background-position: right center;
  background: {
    image: url($svg-select-icon);
    repeat: no-repeat;
    position: right 10px center;
    color: transparent;
    size: 11px auto;
  }
  padding-right: 28px;
  text-indent: 0.01px;
  text-overflow: '';
  cursor: pointer;

  /*================ Hide the svg arrow in IE9 ================*/
  .ie9 & {
    padding-right: 10px;
    background-image: none;
  }
}

select::-ms-expand {
  display: none;
}

/*================ Error styles ================*/
input,
select,
textarea {
  &.input-error {
    border-color: $color_error;
    background-color: $color_error_bg;
    color: $color_error;
  }
}

.form-padding--medium-up{
  @include media-query($medium-up){
    padding-right: 10px;
  }
}

form .errors{
  padding: rem(15px) rem(20px);
  border: 1px solid $alert_border_color;
  border-radius: $radius;
  margin: 2em auto;
  max-width: 400px;
  background-color: $alert_bg_color;
  color: $alert_text_color;

  ul{
    list-style-type: none;
    margin: 10px 20px;
    padding: 0;
    color: inherit;
  }
}

.form-success{
  border: 1px solid $color_site_border;
  border-radius: $radius;
  margin: 0 0 rem(30px) 0;
  padding: rem(15px) rem(20px);
  color: $color_text;
  @include box_shadow(1, $color_site_border);
}

.form--search{
  input{
    width: 80%;
  }
  .btn{
    width: 19%;
    padding: 10px 0;
  }
}

.contact-form .form-success{
  max-width: 400px;
  margin: 30px auto;
}

// captcha page and app pages
.template- .main-content{
  margin-top: 30px;
}

/**
 * Blank States
 */

$color-blankstate: $color_text_light;
$color-blankstate-border: $color_bg_secondary;
$color-blankstate-background: $color_bg_secondary;

.placeholder-svg {
  display: block;
  fill: $color-blankstate;
  background-color: $color-blankstate-background;
  width: 100%;
  height: 100%;
  max-width: 100%;
  max-height: 100%;
  border: 1px solid $color-blankstate-border;
}

.placeholder-svg--small {
  width: 480px;
}

.placeholder-noblocks {
  padding: 40px;
  text-align: center;
}

// Mimic a background image by wrapping the placeholder svg with this class
.placeholder-background {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;

  .icon {
    border: 0;
  }
}

/**
 * Images
 */

.lazy-image{
  display: block;
  position: relative;
  background-size: cover;
}

.lazy-image img {
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: auto;
}

.fade-in.lazyloaded {
  opacity: 1;
}

.fade-in {
  opacity: 0;
  transition: opacity .25s ease-in-out;
}

.no-js{
  .lazyload {
     display: none !important;
   }
  .no-js-image {
    display: block;
  }
  [data-aos]{
    opacity: 1 !important;
    transform: none !important;
  }
}


/*================ MODULES ================*/
/**
 * Header
 */

#header {
  margin: 0;
  padding: 0;
  position: relative;
  z-index: 1000;
  vertical-align:baseline;
  text-align: center;
  background: $color_header_bg;
  color: inherit;

  .wrap {
    position: relative;
    line-height: 1;
    z-index: 99;
  }

  @include media-query($medium-up) {
    top: auto !important;
  }

  @include media-query($small) {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
  }
}

#logo {
  font-size: 28px;
  font-family: $font-stack-heading;
  line-height: 1;
  margin: 0;
  display: block;
  position: relative;
  z-index: 3;
  text-align: center;

  a {
    text-decoration: none;
    color: $color_header_link;
    display: flex;
    margin: auto;
    align-items: center;
    justify-content: center;
    text-align: center;
    padding: 0 20px;
    max-width: 440px;

    img {
      display: block;
      max-width: 100%;
      max-height: 200px;
      height: auto;
      object-fit: contain;

      @include media-query($small) {
        max-height: 100px;
      }
    }

    span {
      display: block;
      padding: .2em 0;
    }
  }

  @include media-query($medium-down) {
    font-size: 24px;
    padding: 0;
    margin: 0 auto;
    max-width: calc(100% - 132px);

    a {
      display: block;
      min-height: 0;
      max-height: 120px;
      overflow: hidden;
      padding: 10px 0;

      img {
        width: auto;
        height: auto;
      }

      span {
        display: block;
        padding: 9px 0;
      }
    }
  }

  @media screen and (max-width: 639px) {
    max-width: calc(100% - 160px);
  }
  @media screen and (max-width: 374px) {
    font-size: 24px;
  }
}

.header__social {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  padding-left: 30px;
  display: flex;
  align-items: center;
  font-size: 20px;

  ul {
    display: flex;

    li {
      margin-right: 18px;
    }
  }

  @include media-query($small) {
    display: none;
  }
}

.header__social--mobile{
  display: none;
  @include media-query($small) {
    border-top: 1px solid $color_menu_border;
    width: 100%;
    display: block;
    background-color: $color_submenu_bg;
    .social__links{
      list-style: none;
      display: flex;
      flex-wrap: wrap;
      margin: 0 0 22px 20px;
      li{
        padding: 22px 30px 0 0;
      }
      svg{
        fill: $color_submenu_link
      }
      a:hover svg{
        fill: $color_submenu_link_hover
      }
    }
  }
}


.header-content {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  top: 0;
  left: 0;
  bottom: 0;
  padding: 12px 30px;
  z-index: 1;
  position: relative;
  @include media-query($small) {
    padding: 0 14px;
    min-height: 46px;
  }
}

.header-content a{
  text-decoration: none;
  color: $color_header_link;
  svg{
    fill: $color_header_link;
  }
  &:hover{
    color: $color_header_link_hover;
    svg{
      fill: $color_header_link_hover;
    }
  }
}

#SearchToggle {
  border: 0;

  * {
    pointer-events: none;
  }
}

.user-links {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  display: flex;
  align-items: center;
  padding-right: 30px;
  font-size: 18px;
  line-height: normal;

  a {
    margin-left: 20px;
    height: 100%;
    display: flex;
    align-items: center;
  }
}
.header__cart__icon__wrapper{
  position: relative;
}
.header__cart__wrapper{
  position: relative;
}
.header__cart__icon{
  font-size: 1.2em;
}
.header__cart__count{
  position: absolute;
  top: -4px;
  right: -9px;
  border: 1px solid $color_header_bg;
  height: 17px;
  min-width: 17px;
  padding: 3px 2px 2px 2px;
  border-radius: 50%;
  line-height: 1em;
  font-size: $font-adjust-body * 0.62em;
  font-weight: $font-weight-body-bold;
  background-color: $color_accent_bright;
  color: $color_header_bg;
  text-decoration: none;
  transition: all ease-out 0.35s;
}
.header__cart__wrapper:hover .header__cart__count{
  color: $color_header_bg;
  transform: scale3d(1.15, 1.15, 1.15);
}

@keyframes pulse-cart-icon {
  from {
    transform: scale3d(1, 1, 1);
  }

  50% {
    transform: scale3d(1.25, 1.25, 1.25);
  }

  to {
    transform: scale3d(1, 1, 1);
  }
}
.pulse-cart-icon {
  animation-name: pulse-cart-icon;
  animation-duration: 0.5s;
  animation-fill-mode: both;
}

.header-content a:hover,
.header-content a:hover span,
.search--open,
.search--open .search-toggle {
  color: $color_header_link_hover;
}
.search-toggle{
  padding-top: 3px;
}

.header__account__link{
  padding-top: 2px;
  font-size: 0.95em;
}

.search {
  @include prefix(transition, max-height 0.2s ease-in-out, webkit moz ms o spec);
  position: relative;
  height: auto;
  max-height: 0;
  width: 100%;
  z-index: 1000;
  background: rgba(0,0,0,0);
  overflow: hidden;

  input:focus{
    outline: none;
  }
  input::placeholder {
    color: $color_header_link;
  }

  .search-query {
    @extend .p;
    padding: 0 80px 0 40px;
    height: 80px;
    color: $color_header_link;
    background: $color_header_bg;
    outline:none;
    margin: 0;
    border: 0;
    width: 100%;
    font-family: $font-stack-body;

    @include media-query( $small ) {
      font-size: 16px;
      line-height: normal;
      white-space: nowrap;
    }
  }

  .remove-search {
    position: absolute;
    right: 30px;
    top: 35px;
    cursor: pointer;
    color: $color_header_bg;
    width: 25px;
    height: 25px;

    &:before,
    &:after {
      content: '';
      position: absolute;
      width: 25px;
      top: 50%;
      left: 0;
      height: 1px;
      background: $color_header_link;
      @include prefix(transform, rotate(45deg), webkit moz spec);
      @include prefix(transition, background 0.2s ease-in-out, webkit moz ms o spec);

      &:hover {
        background: darken( $color_header_link, 10% );
      }
    }
    &:after {
      @include prefix(transform, rotate(-45deg), webkit moz spec);
    }
  }
}
.search--open {
  border-bottom: 1px solid $color_menu_border;
  max-height: 100px;
}

li.has-sub-nav > a .icon {
  margin-left: 9px;
  display: inline-block;
  vertical-align: middle;
  width: 6px;
  height: 10px;
  @include prefix(transform, rotate(90deg), webkit spec);
}

/**
 * DROPDOWNS
 */

/*================ Desktop ================*/

@include media-query($medium-up) {
  .nav__links{
    flex: 0 1 100%;
    display: flex;
    justify-content: center;
    z-index: 1400;
    border-top: 1px solid $color_menu_border;
    border-bottom: 1px solid $color_menu_border;
    background-color: $color_menu_bg;
  }
  .main-menu {
    display: inline-block;
    & > li {
      display: inline-block;
      padding: 14px 5px 11px 5px;
      &:first-child a {
        margin-left: 10px;
      }
      & > a {
        margin-left: 30px;
        padding: 14px 5px;
        white-space: nowrap;
        color: $color_menu_link;
        &:hover{
          color: $color_menu_link_hover;
        }
      }
    }
  }
  .nav-carat-small {
    padding-left: 8px;
  }
  .nav-carat-small svg{
    width: 5px;
    transform: rotateZ(0deg);
    transition: transform 0.25s ease-out;
    fill: $color_menu_link;
  }
  .main-menu > li:hover{
    & > a > .nav-carat-small svg{
      transform: rotateZ(90deg);
      fill: $color_menu_link_hover;
    }
  }

  .grandparent > .main-menu-dropdown > ul > .parent .nav-carat,
  .grandparent > .main-menu-dropdown > ul > .parent .nav-carat-small{
    display: none;
  }
  .nav-carat{
    display: none;
  }
  .main-menu a{
    text-decoration: none;
  }
  .main-menu-dropdown {
    text-align: left;
    ul{ list-style-type: none; }
    li{ padding-top: 8px; }
  }
  /*================ Large Dropdown ================*/
  .main-menu > .parent > .main-menu-dropdown{
    display: none;
    position: absolute;
    z-index: 5000;
    padding-top: 15px;
    background-color: $color_submenu_bg;
    border-bottom: 1px solid $color_menu_border;
    border-left: 1px solid $color_menu_border;
    border-right: 1px solid $color_menu_border;
    padding: 0 40px 20px 30px;
    margin-top: 11px;
    min-width: 130px;
    ul{
      margin: 0;
      padding: 0;
    }
    ul > li > a{
      color: $color_submenu_link;
      &:hover{
        color: $color_submenu_link_hover;
      }
    }
  }
  .main-menu > .parent:hover > .main-menu-dropdown,
  .main-menu > .parent.active > .main-menu-dropdown{
    display: block;
  }
  .main-menu > .parent:first-child > .main-menu-dropdown{
    margin-left: -30px;
  }

  /*================ Large MegaNav ================*/
  .nav{
    position: relative;
  }
  .grandparent > .main-menu-dropdown{
    display: none;
    top: 100%;
    position: absolute;
    background-color: $color_submenu_bg;
    top: calc(100% - 1px);
    width: 100%;
    left: 0;
    margin: 0;
    z-index: 5000;
    border-bottom: 1px solid $color_menu_border;
    padding-top: 30px;
    padding-bottom: 20px;
    a{
      color: $color_submenu_link;
    }
  }
  .grandparent:hover > .main-menu-dropdown,
  .grandparent.active > .main-menu-dropdown{
    display: block;
    .main-menu-dropdown{  display: block; }
  }
  .grandparent > .main-menu-dropdown ul{
    margin: 0 auto;
    max-width: $width-site;
    padding: 10px 30px 20px 30px;
    &:first-child{
      padding-left: 0;
    }
  }
  .grandparent > .main-menu-dropdown > ul > .parent > a,
  .grandparent > .main-menu-dropdown > ul > .child > a{
    width: 100%;
    display: inline-block;
    padding-bottom: 12px;
    border-bottom: 1px solid $color_menu_border;
    font-family: $font-stack-heading;
    font-weight: $font-weight-heading;
  }
  .grandparent a:hover,
  .grandparent .main-menu--active a{
    color: $color_submenu_link_hover !important;
  }
  .grandparent > a{
    position: relative;
    &:hover::after{
      content: '';
      width: 100%;
      bottom: -2px;
      left: 0;
      height: 1px;
      position: absolute;
      z-index: 50;
      background-color: darken($color_menu_border, 50%);
    }
  }


  /* catch the cast where a grandparent has a child */
  .grandparent > .main-menu-dropdown > ul > .parent,
  .grandparent > .main-menu-dropdown > ul > .child{
    float: left;
    min-height: 1px;
    padding-left: 30px;
    padding-top: 0px;
    vertical-align: top;
    & > a{}
  }

  .kids-2.grandparent > .main-menu-dropdown > ul > li,
  .kids-1.grandparent > .main-menu-dropdown > ul > li{
    width: 50%;
    &:nth-child(2n+1){
      clear:both;
    }
  }
  .kids-3.grandparent > .main-menu-dropdown > ul > li,
  .kids-6.grandparent > .main-menu-dropdown > ul > li,
  .kids-9.grandparent > .main-menu-dropdown > ul > li{
    width: 33.333%;
    &:nth-child(3n+1){
      clear:both;
    }
  }
  .kids-4.grandparent > .main-menu-dropdown > ul > li,
  .kids-7.grandparent > .main-menu-dropdown > ul > li,
  .kids-8.grandparent > .main-menu-dropdown > ul > li{
    width: 25%;
    &:nth-child(4n+1){
      clear:both;
    }
  }
  .kids-5.grandparent > .main-menu-dropdown > ul > li,
  .kids-10.grandparent > .main-menu-dropdown > ul > li{
    width: 20%;
    &:nth-child(5n+1){
      clear:both;
    }
  }
}

/*================ Touch ================*/
@include media-query($small) {
  .navigation__wrapper{
    display: none;
    max-height: calc(100vh - 46px);
    overflow: auto;
    -webkit-overflow-scrolling: touch;
  }
  .navigation__wrapper.active{
    display: block;
  }
  .main-menu{
    background-color: $color_submenu_bg;
    margin: 0;
    padding: 0;
    list-style-type: none;
    text-align: left;
    a{
      color: $color_submenu_link;
    }
    .main-menu--active a,
    a:hover{
      color: $color_submenu_link_hover;
    }
  }
  .main-menu li,
  .nav__account{
    position: relative;
    font-size: 17px * $font-adjust-body;
    font-weight: $font-weight-body;
  }
  .nav__account a,
  .nav-link{
    padding: 15px 30px 15px 15px;
    display: inline-block;
  }
  .main-menu-dropdown .nav-link{
    padding: 11px 30px 11px 15px;
    font-size: 14px * $font-adjust-body;
  }
  .nav-carat-small{
    display: none;
  }
  .nav-carat{
    position: absolute;
    right:0;
    top: 3px;
    padding: 10px 15px;
    transform: rotateZ(90deg);
    transition: transform 0.15s ease-out;
    svg{
      width: 15px;
      height: 15px;
      color: $color_submenu_link;
      fill: $color_submenu_link;
    }
    &:hover svg{
      color: $color_submenu_link_hover;
      fill: $color_submenu_link_hover;
    }
    &[aria-expanded="true"]{
      transform: rotateZ(270deg);
      svg{
        color: $color_submenu_link;
        fill: $color_submenu_link;
      }
    }
  }
  .main-menu-dropdown{
    display: none;

    &.expanded{
      display: block;
    }
  }
  .main-menu > li > .main-menu-dropdown {
    border-top: 1px solid $color_menu_border;
    border-bottom: 1px solid $color_menu_border;
    .nav-carat{
      top: 0
    }
  }
  .main-menu-dropdown .main-menu-dropdown{
    margin-left: 15px;
    border-left: 2px solid #f3f3f3;
  }
}




