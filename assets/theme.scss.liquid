/*================ Smart Shopify Payment Buttons ================*/
.product__submit__add{
  height: 44px;
  font-size: 1.05rem * $font-adjust-body;
  margin-bottom: 1em;
  @include media-query($small){
    padding: 7px 14px;
  }
}

.product__submit:not(.product__submit--spb){
  .product__submit__buttons{
    display: inline-block;
  }
  .product__submit__add{
    padding: 8px 60px;
    @include media-query($medium-up){
      height: 45px;
    }
  }
}


.product__submit--spb{
  .quantity-selector{
    display: block;
  }
  .product__submit__add{
    max-width: 400px;
    @extend .btn--secondary;
  }
}

.product__submit--spb .product__submit__buttons{
    margin: 0 0 10px 0;
    display: inline-flex;
    flex-flow: row wrap;
    align-items: flex-start;
    width: 100%;
  .shopify-payment-button,
  .product__submit__add{
    margin: 10px 10px 0 0;
    min-width: 155px;
    flex: auto;
  }
}

.variant--soldout,
.variant--unavailabe{
   .quantity-selector__wrapper,
   .shopify-payment-button,
   .product__price{
     opacity: 0;
     label, input, button{
       cursor: default;
     }
   }
}

.variant--soldout .product__submit__add,
.variant--soldout .product__submit__add:hover{
  border-color: darken($color_bg, 40%);
  background-color: darken($color_bg, 10%);
  color: darken($color_bg, 40%);
  cursor: not-allowed;
}

/*================ Smart Shopify Payment Buttons ================*/
.shopify-payment-button{
  max-width: 400px;
  transition: opacity 0.4s ease-out;
}
.shopify-payment-button .shopify-payment-button__button {
  border-radius: $radius;
  overflow: hidden;
  transition: all .15s ease;
  &:active,
  &:focus {
    transform: translateY(1px);
  }
}
.shopify-payment-button .shopify-payment-button__button--unbranded{
  @extend .btn;
  width: 100%;
  min-height: 44px;
  font-size: 1.05rem * $font-adjust-body;
  &:hover:not([disabled]),
  &:active,
  &:focus {
    background-color: darken($color_accent_bright, 5%);
  }
}

.shopify-payment-button .shopify-payment-button__more-options{
  font-weight: bold;
  font-size: 0.9rem * $font-adjust-body;
  color: $color_text;
  &:active{
    text-decoration: underline;
    outline: none;
    transform: translateY(1px);
  }
}

.product__sharing {
