}
.product__add__success--hide{
  height: 0px;
  opacity: 0;
  overflow: hidden;
}

@keyframes fadeInDown {
  from {
    opacity: 0;
    transform: translate3d(0, -100%, 0);
  }

  to {
    opacity: 1;
    transform: translate3d(0, 0, 0);
  }
}

.fadeInDown {
  animation-name: fadeInDown;
  animation-duration: 0.5s;
  animation-fill-mode: both;
}


/*====== Video button =======*/
.product__photo{
  position: relative;
}
.product__play{
  z-index: 100;
  transition: opacity 0.2s ease-out;
  &:hover{
    opacity: 0.75;
  }
  .icon{
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -25px;
    margin-left: -25px;
    width: 50px;
    height: 50px;
    fill: #fff;
    -webkit-filter: drop-shadow(1px 1px 3px rgba(0,0,0,0.2));
    filter: drop-shadow(1px 1px 3px rgba(0,0,0,0.2));
  }
}

/**
 * Cart
 */

.cart {
  width: auto;
  padding: 0 20px;
  position: relative;
  margin: 40px auto 0;
  overflow: visible;
  display: block;
  text-align: left;
  max-width: 1100px;
}
.cart .theme {
  text-align: left;
}
.cart .empty .theme {
  text-align: center;
}
.cart a {
  text-decoration: none;
}
.cart .empty {
  text-align: center;
}
.cart table {
  margin: 20px 0px;
  width: 100%;
  border: 0;
  table-layout: fixed;
}
.cart table th {
  @extend .p;
  font-size: 1em;
  border-right: 0;
  color: $color_text;
}
.cart table thead th:last-child {
  text-align: right;
}
.cart table tbody td {
  @extend .p;
  font-size: 1em;
  border-top: none;
  border-left: none;
  border-right: none;
  padding: 10px 4px;
  vertical-align: middle;
  font-weight: $font-weight-body;

  @media (max-width: 480px){
    padding: 20px 0;
  }

  &.desc {
    word-break: break-word;

    small {
      display: block;
      font-size: 0.875em;
      opacity: .7;
    }
  }
}
.cart table tbody td:last-child {
  text-align: right;
}
.cart td.properties {
  padding-top: 0 !important;
}
.cart table tbody td input:focus {
  outline: none;
}
.cart table tbody td a.remove-from-cart {
  @extend .small;
  font-size: 0.8em;
  text-transform: uppercase;
  opacity: .7;
}
.cart table tbody td a.remove_from_cart:hover {
  color: white;
}
.cart table tbody td img {
  max-height: 50px;
  max-width: 100%;
}
// CART LINE ITEM PROPS
.cart table {
  @include media-query($small) {
    table-layout: auto;

    tbody td.item td.quantity {
      text-align: right;

    }
    tbody td.quantity {
      padding-right: 10px;
    }

    tbody td.item td.img {
      padding-right: 25px;
      img {
        margin-bottom: 5px;
      }
    }
  }

  tr.item.with-props {
    td {
      border-bottom: 0;
    }
  }

  input {
    font-size: 1em;
  }
}
.cart table tbody tr.props {
  .__props {
    display: block;
    position: relative;
    border-left: 2px solid $color_accent_bright;
    padding: 4px 0 4px 15px !important;
  }
  td {
    border-bottom: 0;
    padding: 5px 0 10px !important;
    text-align: left;
  }
  & + tr.item td {
