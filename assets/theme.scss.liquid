.article__content {
  padding: 14px 0 0;
  font-size: 18px;
  line-height: 26px;

  p {
    font-size: inherit;
    line-height: inherit;
    margin: 0 0 24px;
  }

  blockquote {
    font-style: italic;
    margin: 52px 0;
    padding: 0 0 0 28px;
