}

.widget__links {
  margin: 0;
  font-weight: bold;
  font-size: 14px;
  line-height: 17px;
  li {
    list-style: none;
  }

  a {
    display: block;
    color: $color_link;
    padding: 6px 0;

    @include prefix(transition, opacity .2s ease-out, webkit moz ms o spec);

    &:hover {
      color: $color_link_hover;
    }
  }

  .has-sub-nav {
    & > a {
      position: relative;
      padding: 6px 24px 6px 0;

      &::after {
        font-family: 'Icons';
        content: '\e034';
        position: absolute;
        top: 0;
        right: 0;
        bottom: 0;
        display: flex;
        align-items: center;
        padding: 0 14px 0 0;
        font-size: 12px;
      }
