  .submenu {
    font-weight: normal;
    margin: 4px 0 4px 2px;
    padding: 0 0 0 15px;
    border-left: 2px solid $color_site_border;
    display: none;

    li.active {
      a {
        opacity: 1;
      }
    }
  }

  @include media-query($small) {
    .has-sub-nav > a::after {
      content: none;
      display: none;
    }

    .submenu {
      display: none !important;
    }
  }
}

.widget--categories {
  @include media-query($small) {
    border-bottom: 1px solid $color_site_border;
    margin: 0;

    .widget__title {
      font-size: 14px;
      padding: 9px 10px;
      text-align: center;
      margin-bottom: 0;
    }

    .widget__links {
      text-align: center;
      height: 33px;
      overflow: hidden;
      z-index: 1;

      li {
        margin: 0;

        a {
          padding: 8px 10px;
          font-weight: normal;
          opacity: 1;
        }
      }

      .slick-arrow {
        margin: 0;
        top: 0;
        bottom: 0;
        color: $color_text;
        background: $color_bg;
        box-shadow: none;
        font-size: 21px;
        line-height: 1;
        display: flex;
        align-items: center;
        opacity: .7;
        @include prefix(transform, none, webkit moz ms o spec);
      }

      .slick-prev {
        left: 0;
        padding-left: 12px;

        svg {
          @include prefix(transform, rotateY(-180deg), webkit mz o spec);
        }
      }

      .slick-next {
        right: 0;
        padding-right: 12px;
      }
    }
  }
}

.widget__links--active a{
  color: $color_accent_bright;
}

.widget--pinterest {
  .widget__image {
    margin-top: 20px;
    position: relative;
    overflow: hidden;
    img {
      display: block;
      width: 100%;
      height: auto;
      border-radius: 10px;
    }
  }
}

// article-column-item
.widget__recent{
  margin-top: 30px;
}
.article__column{
  display: flex;
  align-items: center;
  justify-content: flex-start;
  text-align: left;
  position: relative;
  min-height: 120px;
  margin-bottom: 30px;
  transition: all 0.3s ease-out;
  border: 1px solid $color_site_border;

  &:last-child {
    margin-bottom: 0;
  }

  .meta--light{
    margin-top: 8px;
    font-size: rem(14px * $font-adjust-body);
    color: lighten($color_text_light, 15%);

    @include media-query($medium) {
      color: lighten($color_text_light, 100%);
    }
  }
  &:hover{
    border: 1px solid darken($color_site_border, 3%)
  }
}
.article__column__image__wrapper{
  img{
    display: block;
  }
  width: 33%;

  @include media-query($medium) {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
  }
}
.article__column__contents{
  padding: 10px 10px 10px 5%;
  flex: 1;
  @include media-query($medium) {
    position: relative;
    z-index: 2;
  }
}
.article__column__title{
  margin: 0 0 9px 0;
  color: $color_text;

  @include media-query($medium) {
    color: lighten($color_text_light, 100%);
  }
}
.article__column__image{
  display: block;
  background-size: cover;
  background-position: center center;
  width: 33%;
  height: 100%;
  overflow: hidden;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;

  @include media-query($medium) {
    width: 100%;
  }
}

/**
 * Js dropdown
 */


.js-dropdown {
  padding: 0;
  text-align: left;
  min-height: 0;
  border: 0;
  position: relative;
  overflow: visible;
  display: block;
}
.js-dropdown__label {
  display: block;
  padding: 15px;
  cursor: pointer;
  position: relative;
  text-transform: capitalize;
  border-bottom: 1px solid $color_site_border_light;
}
.js-dropdown__label::after {
  font-family: 'Icons';
  content: '\e02d';
  position: absolute;
  right: 15px;
  font-size: 75%;
  top: 50%;
  z-index: 10;
  @include prefix(transform, translateY(-50%), webkit moz ms o spec);
}
.js-dropdown__list {
  padding: 0;
  position: absolute;
  z-index: 99;
  background: $color_bg_secondary;
  top: 100%;
  left: 0;
  right: 0;
  max-height: 0;
  overflow: hidden;
  transition: max-height 0.5s ease-in;
}
.js-dropdown__list.visible {
  max-height: 400px;
  overflow-y: scroll;
}
.js-dropdown__list li {
  display: block;
  border-top: 1px solid $color_site_border;
  padding: 15px;
  cursor: pointer;
  &:hover {
    color: $color_link_hover;
    background: $color_bg;
  }

  &.active {
    a {
      box-shadow: none;
    }
  }
}


/**
 * Collections
 */

.template-collection .container{
  background-color: rgba(255,255,255,0);
}
.collection__nav,
.product-grid{
  background-color: $color_bg;
}

.filter-tag {
  position:relative;
  margin:0;
  padding:0;
  overflow:visible;
  display:block;
  padding:0px 30px;
  min-height:45px;
  text-align:center;
}
.filter-tag ul {
  position:relative;
  margin:0;
  padding:0;
  list-style:none;
  text-align: left;
}
.filter-tag li {
  position:relative;
  margin:0;
  padding:0;
  display:inline-block;
  text-decoration:none;
  color: $color_text_light;
  font-weight: $font-weight-body-bold;
}
.filter-tag li a {
  font-family: $font-stack-heading;
  text-decoration:none;
  color: $color_text_light;
  -webkit-transition: color 0.15s ease-in;
  -moz-transition: color 0.15s ease-in;
  -ms-transition: color 0.15s ease-in;
  -o-transition: color 0.15s ease-in;
  transition: color 0.15s ease-in;
  padding: 15px;
  display:block;
}
.filter-tag li.back a {
  margin-left:0px;
}
.filter-tag li a:hover,
.filter-tag li.active a {
  text-decoration:none;
  color: $color_text_dark;
}
.filter-tag li.active a {
  box-shadow: 0 2px $color_text_dark;
}

.collection__sorting{
  width: 200px;
  display: flex;
  align-items: flex-end;
  .js-dropdown{
    width: 200px;
  }
}

@include media-query($small) {
  .collection__sorting{
    width: 100%;
    .js-dropdown{
      width: 100%;
    }
  }
}

@include media-query($medium-up) {
  .collection__nav--sort{
    display: flex;
    justify-content: space-between;
  }
  .collection__nav{
    background: $color_bg_secondary;
  }
}

/**
 * COLLECTION PAGE HEADER
 */

.collection__title{
  .hero__title{
    padding: 0;
    margin: 0;
  }
  .hero__description{
    padding: 0;
    margin: 0;
  }
  .hero__title + .hero__description{
    margin-top: 0.5em;
  }
}
.collection__title:not(.hero__content__wrapper){
  padding: 30px;
  @include media-query($small) {
    padding: 25px 15px;
  }
}

.filter-tag--mobile {
  display: none;
}

@include media-query($small) {
  .filter-tag {
    padding: 0;
    text-align: left;
    min-height: 0;
    border: 0;
  }
  .filter-tag--mobile {
    display: block;
    padding: 15px;
    cursor: pointer;
    position: relative;
    text-transform: capitalize;
    border-bottom: 1px solid $color_site_border_light;
  }
  .filter-tag--mobile::after {
    font-family: 'Icons';
    content: '\e02d';
    position: absolute;
    right: 15px;
    font-size: 75%;
    top: 50%;
    z-index: 10;
    @include prefix(transform, translateY(-50%), webkit moz ms o spec);
  }
  .filter-tag ul {
    position: absolute;
    z-index: 99;
    background: $color_bg_secondary;
    top: 100%;
    left: 0;
    right: 0;
    max-height: 0;
    overflow: hidden;
    -webkit-transition: max-height 0.5s ease-in;
    -moz-transition: max-height 0.5s ease-in;
    -ms-transition: max-height 0.5s ease-in;
    -o-transition: max-height 0.5s ease-in;
    transition: max-height 0.5s ease-in;
  }
  .filter-tag ul.visible {
    max-height: 400px;
    overflow-y: scroll;
  }
  .filter-tag li {
    display: block;
    border-top: 1px solid $color_site_border;
    font-weight: $font-weight-body;
    font-family: $font-stack-body;

    &.active {
      a {
        box-shadow: none;
      }
    }
  }
}

@include media-query($small) {
  .slideup {
    display: none !important;
  }
}

// PAGINATION
.pagination {
  text-align: center;
  padding: 20px 0;
}
.pagination span {
  margin: 0 5px;
}
.pagination .current {
  font-weight: $font-weight-body-bold;
}
.pagination a {
  text-decoration: none;
}


.product-grid--search{
  .product-item{
    width: 33.33%;
    @include media-query($small){
