  width: 15px;
  height: 15px;
  background-color: #fff;
  border-radius: 10px;
  &::after{
    content: "";
    width: 25px;
    height: 25px;
    background-color: inherit;
    border-radius: 25px;
    opacity: 0.3;
    position: absolute;
    top: -5px;
    left: -5px;
  }
  &::before{
    content: "";
    width: 25px;
    height: 25px;
    background-color: inherit;
    border-radius: 25px;
    opacity: 0.3;
    position: absolute;
    top: -5px;
    left: -5px;
    animation: pulse 2.5s linear;
    animation-iteration-count: infinite;
