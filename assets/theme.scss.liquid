/**
 * Footer
 */


.site-footer-wrapper {
  position: relative;
  background-color: $color_footer_bg;
  color: $color_footer_text;
  text-align: left;
  hr {
    border-top: 1px solid rgba($color_footer_text, 0.5);
  }
  a{
    color: $color_footer_link;
