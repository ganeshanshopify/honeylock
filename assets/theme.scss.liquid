/**
 * Product Grid
 */

.product-grid {
  display: flex;
  flex-wrap: wrap;
  border-left: 1px solid $color_site_border;
  padding-top: 1px;
  -webkit-overflow-scrolling: touch;
  scroll-snap-type: mandatory;
  scroll-snap-destination: 0% 100%;
  scroll-snap-points-x: repeat(100%);

  .flickity-button:disabled {
    opacity: 0;
  }
}

.product-item {
  position: relative;
  border: 1px solid $color_site_border;
  border-left: 0;
  background: #fff;
  margin-top: -1px;
  min-height: 100%;
  scroll-snap-align: start;

  .product-link {
    display: block;
    height: 100%;
    color: $color_text;
  }

  .sale-box {
    position:absolute;
    left:0;
    top:0;
    z-index: 3;
    height:35px;
    width:80px;
    overflow:hidden;
    line-height:35px;
    text-align:center;
    color: $color_sale_text;
    background: $color_sale_bg;
    @extend .caps;
  }
}

.product-item__image {
  display: block;
  padding-top: $product_grid_aspect_ratio;
  height: 0;
  position: relative;
  overflow: hidden;
  border-bottom: 1px solid $color_site_border;
}
.product-item__bg{
  position: absolute;
  display: block;
  top: 0;
  height:100%;
  width: 100%;
  overflow: hidden;
  background-size: cover;
  background-position: center center;
}

.product-information {
  padding: 20px;
  text-align: center;
  position: relative;

  h3 {
    font-weight: normal;
    font-size: 18px;
    line-height: 1.4;
    margin: 7px 0;
  }

  .price {
    font-size: 14px;
    opacity: .7;

    &.sale {
      opacity: 1;
    }
  }


  .item-type {
    color: $color_text;
  }

  .spr-icon {
    padding: 0 1px;
  }

  .spr-badge-starrating {
    display: block;
    margin: 3px 0 5px;
    font-size: 12px;
    font-weight: normal;
    color: $color_link;
  }

  .spr-badge-caption {
    display: none;
  }

  .sold-out {
    color: $color_sale_bg;
  }
}
.old-price {
  color: $color_text;
  text-decoration: line-through;
  opacity: .5;
  margin-right: 10px;
}
.price.sale {
  color: $color_sale_bg;
}

.no-results {
  padding: $gutter 30px;
  text-align: center;
  width: 100%;
}

@include media-query($small) {
  .product-grid--horizontally {
    position: relative;
    display: block;
    backface-visibility: hidden;
    -webkit-backface-visibility: hidden;

    .product-item {
      min-width: 250px;
    }
  }

  .product-grid--vertically {
    .product-item {
      width: 100%;
    }
  }
}


.no-touch .double__image{
  .product-item__bg__under{ opacity: 0; }
  &:hover{
    .product-item__bg__under{ opacity: 1; }
  }
}
.touch .product-item__bg__under{
  display: none;
}

.product-item__bg__under{
  display: block;
  position: absolute;
  top: 0;
  right: 0;
  z-index: 1;
  width: 100%;
  height: 100%;
  background-size: cover;
  background-position: center center;
  overflow: hidden;
  -webkit-transition: opacity .25s ease-in-out;
  -moz-transition: opacity .25s ease-in-out;
  -o-transition: opacity .25s ease-in-out;
  transition: opacity .25s ease-in-out;
}



// product-column-item
.widget__products{
  margin-top: 30px;
}
.product__column{
  display: flex;
  align-items: center;
  justify-content: flex-start;
  text-align: left;
  position: relative;
  min-height: 120px;
  margin-bottom: 30px;
  transition: all 0.3s ease-out;
  border: 1px solid $color_site_border;
  .meta--light{
    margin-top: 8px;
    font-size: rem(14px * $font-adjust-body);
    color: lighten($color_text_light, 15%)
  }
  &:hover{
    border: 1px solid darken($color_site_border, 3%)
  }
}
.product__column__image__wrapper{
  img{
    display: block;
  }
  width: 33%;
}
.product__column__contents{
  padding: 10px 10px 10px 5%;
  flex: 1;
}
.product__column__title{
  margin: 0 0 9px 0;
  color: $color_text;
}
.product__column__image{
  display: block;
  background-size: cover;
  background-position: center center;
  width: 33%;
  height: 100%;
  overflow: hidden;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}

/**
 * Quick View Modal
 */

// button styles

.slideup {
  position: absolute;
  bottom: -60px;
  -webkit-transition: bottom 0.25s ease-in;
  -moz-transition: bottom 0.25s ease-in;
  -ms-transition: bottom 0.25s ease-in;
  -o-transition: bottom 0.25s ease-in;
  transition: bottom 0.25s ease-in;
  z-index: 9;
  width: 100%;
  background: $color_bg_secondary;
  opacity: 0.8;
  border-top: 1px solid $color_site_border;
}
.product-item__image {
  .slideup {
    position: absolute;
    opacity: 0;
    text-align: center;
    transition: opacity .2s ease-in-out, bottom .2s ease-in-out;
    span:hover {
      color: inherit;
    }
  }

  &:hover {
    .slideup {
      bottom: 0px;
      opacity: .9;
    }
  }
}
.no-touch .slideup.up {
  bottom: 0;
}
.slideup span {
  text-decoration: none;
  display: block;
  color: $color_text;
  padding: 20px;
}
.no-touch .slideup span:hover {
  color: $color_link;
}

// Modal styles

.modal__inner__wrapper {
  background: $color_bg;
  padding: 50px;
  max-width: 960px;
  max-height: 100%;
  overflow-x: hidden;
  overflow-y: auto;
  margin: 0 auto;
  position: relative;
}

.modal--product .quantity-selector{
  margin-bottom: 15px;
}

// hide photos until slideshow initializes to avoid jump
.modal--product .product__slides{
  visibility: hidden;
  height: 1px;
}
.modal--product .product__slides.slick-initialized{
  visibility: visible;
  height: auto;
}
.modal--product .product__slides.product__slides--single{
  visibility: visible;
  height: auto;
}

/**
 * Product Page
 */

.product__wrapper{
  margin: 90px 40px 0 40px;
  @include media-query($small) {
   margin-top: 0;
   margin: 90px 15px 0 15px;
  }
}
.breadcrumbs{
