    display: block;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    width: 48px;
    transform: rotate(0deg);
    cursor: pointer;
    overflow: hidden;
    z-index: 9999;
  }

  .toggle-menus-lines {
    display: block;
    position: absolute;
    top: 50%;
    right: 7px;
    margin: -12px 0 0;
    width: 34px;
    height: 24px;
    transition: .5s ease-in-out;

    span {
      display: block;
      position: absolute;
      height: 3px;
      width: 50%;
      background: $color_header_link;
      opacity: 1;
      transform: rotate(0deg);
      transition: .25s ease-in-out;

      &:nth-child(even) {
        left: calc(50% - 7px);
        border-radius: 0 3px 3px 0;
      }

      &:nth-child(odd) {
        left: 7px;
        border-radius: 3px 0 0 3px;
      }

      &:nth-child(1),
      &:nth-child(2) {
        top: 3px;
      }

      &:nth-child(3),
      &:nth-child(4) {
        top: 10px;
      }

      &:nth-child(5),
      &:nth-child(6) {
        top: 17px;
      }
    }
  }

  a.toggle-menus--open {
    span {
      background: $color_header_link;

      &:nth-child(1),
      &:nth-child(6) {
          transform: rotate(45deg);
      }

      &:nth-child(2),
      &:nth-child(5) {
          transform: rotate(-45deg);
      }

      &:nth-child(1) {
          left: 7px;
          top: 9px;
      }

      &:nth-child(2) {
          left: calc(50% - 7px);
          top: 9px;
      }

      &:nth-child(3) {
          opacity: 0;
      }

      &:nth-child(4) {
          opacity: 0;
      }

      &:nth-child(5) {
          left: 7px;
          top: 12px;
      }

      &:nth-child(6) {
          left: calc(50% - 7px);
          top: 12px;
      }
    }
  }

  .header-content {
    z-index: 5;
  }

  .user-links {
    left: 15px;
    justify-content: flex-start;

    a {
      margin: 0 16px 0 0;

      &.user-link--icon {
        font-size: 0;
        line-height: 0;
        text-indent: -100%;
        overflow: hidden;
        position: relative;
        display: inline-block;
        height: 22px;
      }
    }
  }

  #SearchToggle {
    order: 1;
  }
  #account-link {
    order: 2;
  }

  #customer_logout_link {
    order: 3;
  }

  #account-links {
    display: none;
  }

  #CartButton {
    position: absolute;
    right: 48px;
    padding: 0 10px;
    margin: 0;
  }

  .touch .header-content a.toggle-menus {
    @include prefix(transition, right 0.4s ease-in-out, webkit moz ms o spec);
  }

  // SEARCH
  .search {
    .search-query {
      height: 60px;
      padding: 0 40px 0 15px;
    }

    .remove-search {
      right: 15px;
      top: 15px;
      width: 30px;
      height: 30px;

      &:before,
      &:after {
        width: 30px;
      }
    }
  }
}

